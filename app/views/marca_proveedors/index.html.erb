<p id="notice"><%= notice %></p>

<h1>Listing Marca Proveedors</h1>

<table>
  <thead>
    <tr>
      <th>Marca</th>
      <th>Proveedore</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @marca_proveedors.each do |marca_proveedor| %>
      <tr>
        <td><%= marca_proveedor.marca %></td>
        <td><%= marca_proveedor.proveedore %></td>
        <td><%= link_to 'Show', marca_proveedor %></td>
        <td><%= link_to 'Edit', edit_marca_proveedor_path(marca_proveedor) %></td>
        <td><%= link_to 'Destroy', marca_proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Marca proveedor', new_marca_proveedor_path %>
